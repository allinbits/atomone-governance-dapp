{
  "name": "@eclesia/atomone-governance",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "queries": "npx graphql-code-generator -c codegen.ts",
    "build": "vue-tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test:e2e:serve": "serve -s dist -l 8080",
    "test:e2e": "npx playwright install && playwright test",
    "test:components": "npx playwright install && playwright test -c playwright-ct.config.ts",
    "test:components:debug": "npx playwright install && playwright test -c playwright-ct.config.ts --debug",
    "test:unit": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@agoose77/markdown-it-mermaid": "^1.1.0",
    "@apollo/client": "^3.11.8",
    "@cosmjs/proto-signing": "^0.36.0",
    "@cosmjs/stargate": "^0.36.0",
    "@cosmostation/cosmos-client": "^0.0.5",
    "@giscus/vue": "^3.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@keplr-wallet/types": "^0.12.272",
    "@tailwindcss/typography": "^0.5.16",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vue/apollo-composable": "^4.2.2",
    "@vueuse/core": "^13.9.0",
    "@wdns/vue-code-block": "^2.3.5",
    "chart.js": "^4.5.0",
    "chartjs-chart-treemap": "^3.1.0",
    "cosmjs-types": "^0.10.1",
    "dayjs": "^1.11.18",
    "dompurify": "3.2.6",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "markdown-it": "^14.1.0",
    "markdown-it-github-alert": "^1.0.6",
    "markdown-it-katex": "^2.0.3",
    "mitt": "^3.0.1",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@atomone/atomone-types": "^1.0.3",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.2",
    "@playwright/experimental-ct-vue": "1.55.0",
    "@playwright/test": "1.55.0",
    "@rollup/plugin-graphql": "^2.0.5",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.5.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.10",
    "@vitest/ui": "^3.2.4",
    "@vue/compiler-dom": "^3.5.21",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "bignumber.js": "^9.3.1",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-plugin-playwright": "^2.2.2",
