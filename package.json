    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  },
  "pnpm": {
    "overrides": {
      "mermaid": "^11.11.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "protobufjs",
      "vue-demi"
    ]
  }
}
